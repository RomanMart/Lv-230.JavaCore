}
System.out.println("Our numbers are " + myCollection);

	ArrayList newCollection = new ArrayList();
	newCollection = myCollection;
	

Comparator c = null;
newCollection.sort(c);
	for(int i = 0;i < newCollection.size(); i++){
		
	}
	System.out.println(newCollection);

	
	int x1 = 0;
	int n = 0;
